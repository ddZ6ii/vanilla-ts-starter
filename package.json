    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "path": "^0.12.7",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4",
    "url": "^0.11.3"
